{
  "parserOptions": {
    "ecmaVersion": 5
  },
  "env": {
    "browser": true,
    "es6": false
  },
  "rules": {
    "no-await-in-loop": 0,
    "no-console": [ 2, { "allow": [ "warn", "error" ] } ],
    "indent": [ 2, 2 ]
  },
  "globals": {
    "PDFNet": true,
    "PDFTron": true,
    "WebViewer": true,
    "saveBufferAsPNG": true,
    "saveBufferAsPDFDoc": true,
    "saveBufferAsXOD": true,
    "saveBuffer": true
  }
}